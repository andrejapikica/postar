<?xml version="1.0" encoding="UTF-8"?>
<definitions 
		xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/" 
		xmlns:tns="a1"
		xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
        xmlns="http://schemas.xmlsoap.org/wsdl/"
		targetNamespace="a1">

	<types>
		<xsd:schema targetNamespace="a1">
			<xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/" />
			<xsd:import namespace="http://schemas.xmlsoap.org/wsdl/" />
			<xsd:complexType name="Subject">
				<xsd:all>
					<xsd:element name="ids" type="xsd:integer"/>
					<xsd:element name="name" type="xsd:string"/>
					<xsd:element name="surname" type="xsd:string"/>
					<xsd:element name="address" type="xsd:string"/>
					<xsd:element name="postCodeName" type="xsd:string"/>
					<xsd:element name="taxNumber" type="xsd:integer"/>
				</xsd:all>
			</xsd:complexType>
		</xsd:schema>
	</types>

	<message name="saveBilRequest">
		<part name="subject" type="tns:Subject"/>
	</message>

	<message name="saveBilResponse">
		<part name="return" type="xsd:string"/>
	</message>

	<portType name="BilPort">
		<operation name="saveBil">
			<input message="tns:saveBilRequest"/>
			<output message="tns:saveBilResponse"/>
		</operation>
	</portType>

	<binding name="BilBinding" type="tns:BilPort">
		<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="saveBil">
			<soap:operation soapAction="uri:a1/saveBil" style="rpc" />
			<input>
				<soap:body use="encoded" namespace="uri:a1" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</input>
			<output>
				<soap:body use="encoded" namespace="uri:a1" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
			</output>
		</operation>
	</binding>

	<service name="Bil">
		<port name="BilPort" binding="tns:BilBinding">
			<soap:address location="https://incubator.alcyone.si/soaptest/bil/bil.php"/>
		</port>
	</service>

</definitions>